<div class="container">
<div class="row h-100 center">
    <div class="col-md-4"></div>
      <div class="col-md-4 center"> 
        <h5>New schedule</h5>
          <form action="/user/agendardata" method="POST">
            <div class="form-group">
                <select name="idMan" id="idMan" class="material_select">
                    <option value="" disabled selected>Choose the store</option>
                     {{#each st }} 
                    <option value="{{this._id}}">{{this.nameStore}}</option>
                    {{/each}}
                </select>
            <button class="mt-1 btn btn-primary" type="submit">Next</button>
          </form>
      </div>
    <div class="col-md-4"></div>    
</div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
 <script>
  $(document).ready(function(){
    $('.datepicker-newSchedule').datepicker({
      selectMonths: true,
      selectYears: 15,
      max: +60
    });
  });

  $(document).ready(function(){
    $('.timepicker-newSchedule').timepicker();
  });

  $(document).ready(function() {
    $('select').formSelect();
  });

 </script>