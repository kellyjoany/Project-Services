<div class="container">
  <div class="row">
      <div class="col s2"></div>
        <div class="col s8 my-auto"> 
        <h5>Schedules done</h5>
          <form action="/showSchedules" method="post">
              <select name="idStoreSchedule" class="material_select">
                    <option value="" disabled selected>Choose the adress</option>
                    {{#each address }} 
                    <option value="{{this._id}}">{{this.address}}</option>
                    {{/each}}
              </select>   
              <br>      
              <button class="btn btn-primary" type="submit">Search</button>
          </form>    
      </div>
    <div class="col s2"></div>    
</div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
 <script>
  $(document).ready(function(){
    $('.datepicker-newSchedule').datepicker({
      selectMonths: true,
      selectYears: 15,
      max: +60
    });
  });

  $(document).ready(function(){
    $('.timepicker-newSchedule').timepicker();
  });

  $(document).ready(function() {
    $('select').formSelect();
  });

 </script>